[user]
  name = Sirut Buasai
  email = sirutbuasai27@hotmail.com

[github]
  user = sirutBuasai

[alias]
  a = add
  aa = add .
  ac = commit -am
  b = branch
  cm = commit
  cmm = commit -m
  cl = clone
  co = checkout
  dc = diff --cached
  df = diff
  dh = diff HEAD^
  f = fetch
  pl = pull
  plo = pull origin
  po = push origin
  pom = push origin master
  pu = push
  re = restore
  res = restore --staged
  rhard = reset --hard HEAD~
  rstg = reset --mixed HEAD~
  rsoft = reset --soft HEAD~
  st = status
  tree = log --graph --date=local --pretty=format:'%Cgreen%ad%Creset %Cred%h%Creset %an: %s %Creset %C(yellow)%d%Creset' --all

[color]
  branch = auto
  diff = auto
  ui = auto
  status = auto

[credential]
  helper = osxkeychain

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red
  new = green

